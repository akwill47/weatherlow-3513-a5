<AnchorPane prefHeight="142.0" prefWidth="216.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.PopUpController">
   <children>
      <TextField fx:id="addSearch" layoutX="43.0" layoutY="59.0" promptText="Enter search key" />
      <Text layoutX="7.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter your search key in the box below">
         <font>
            <Font size="13.0" />
         </font>
      </Text>
      <Button fx:id="close_btn" layoutX="80.0" layoutY="103.0" mnemonicParsing="false" onAction="#closeBtnClick" text="search" />
   </children>
</AnchorPane>
